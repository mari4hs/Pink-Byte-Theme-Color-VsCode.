	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.comment"
			],
			"settings": {
				"foreground": "#71d3f1",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"comment.todo",
				"comment.fixme",
				"comment.note",
				"comment.warning"
			],
			"settings": {
				"foreground": "#FF4E8C",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.language.boolean",
				"constant.other",
				"constant.language",
				"constant.other.null",
				"constant.other.undefined"
			],
			"settings": {
				"foreground": "#fda6b7"
			}
		},
		{
			"scope": [
				"constant.numeric.string"
			],
			"settings": {
				"foreground": "#FFB7C5",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"constant.numeric.hex",
				"constant.numeric.octal",
				"constant.numeric.binary"
			],
			"settings": {
				"foreground": "#FFB7C5",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"string",
				"string.quoted",
				"string.template",
				"string.attribute-value.html",
				"string.quoted.double.html"
			],
			"settings": {
				"foreground": "#FFA6C9"
			}
		},
		{
			"scope": [
				"string.quoted.multi",
				"string.template"
			],
			"settings": {
				"foreground": "#FFA6C9",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"variable",
				"variable.other",
				"support.variable",
				"meta.definition.variable.name",
				"meta.property-name"
			],
			"settings": {
				"foreground": "#c20b2c98"
			}
		},
		{
			"scope": [
				"variable.parameter",
				"variable.language"
			],
			"settings": {
				"foreground": "#da8897",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"variable.local",
				"variable.other.readwrite"
			],
			"settings": {
				"foreground": "#FFB7C5"
			}
		},
		{
			"scope": [
				"keyword",
				"storage",
				"storage.type",
				"keyword.control",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#CBA0FF",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
